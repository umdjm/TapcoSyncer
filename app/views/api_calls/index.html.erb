<h1>Listing api_calls</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Call time</th>
    <th>Result</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @api_calls.each do |api_call| %>
  <tr>
    <td><%= api_call.url %></td>
    <td><%= api_call.call_time %></td>
    <td><%= api_call.result %></td>
    <td><%= link_to 'Show', api_call %></td>
    <td><%= link_to 'Edit', edit_api_call_path(api_call) %></td>
    <td><%= link_to 'Destroy', api_call, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Api call', new_api_call_path %>
